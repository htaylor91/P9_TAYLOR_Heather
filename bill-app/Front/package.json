{
    "scripts": {
    "test": "jest",
    "test coverage": "jest --coverage",
    "test coverage silent": "jest --coverage --noStackTrace --silent",
    "test coverage watch": " jest --coverage --watch"
  },
  "jest": {
    "verbose": true,
    "setupFiles": [
      "./setup-jest.js"
    ],
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/app/**",
      "!**/assets/**",
      "!**/external/**",
      "!**/fixtures/**",
      "!**/lcov-report/**"
    ]
  },
  "type": "module",
  "dependencies": {
    "express": "^4.18.2",
    "global": "^4.4.0",
    "jquery": "^3.6.3",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@testing-library/dom": "^7.31.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^29.2.3",
    "babel-jest": "^27.0",
    "jest": "^27.5.1",
    "jest-environment-jsdom": "^27.4.6",
    "jest-html-reporter": "^3.4"
  }
}